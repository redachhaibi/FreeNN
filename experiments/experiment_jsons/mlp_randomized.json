{
    "dataset": {
        "name": "MNIST",
        "mininatch_size": 512,
        "num_workers": 8
    },
    "network": {
        "build_type": "from_list",
        "build_kwargs": {
            "layer_description": [
                {
                    "in_size": 784,
                    "out_size": 32,
                    "bias": true,
                    "non_linearity": "ReLU"
                },
                {
                    "in_size": 32,
                    "out_size": 27,
                    "bias": true,
                    "non_linearity": "ReLU"
                },
                {
                    "in_size": 27,
                    "out_size": 15,
                    "bias": true,
                    "non_linearity": "ReLU"
                },
                {
                    "in_size": 15,
                    "out_size": 10,
                    "bias": true,
                    "non_linearity": "LogSoftmax"
                }
            ]
        }
    },
    "initialization": {
        "name": "xavier_normal_",
        "torch_kwargs": {}
    },
    "reporters": [
        {
            "period": 10,
            "variable": "weights"
        },
        {
            "period": 10,
            "variable": "weights_gradients"
        },
        {
            "period": 10,
            "variable": "biases"
        },
        {
            "period": 10,
            "variable": "biases_gradients"
        }
    ],
    "trainer": {
        "loss_name": "CrossEntropyLoss",
        "optimizer_kwargs": {
            "name": "SGD",
            "lr": 0.001,
            "momentum": 0.9
        },
        "nb_epochs": 10
    }
}